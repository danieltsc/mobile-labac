{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@logic/*": ["src/logic/*"],
      "@navigation/*": ["src/navigation/*"],
      "@screens/*": ["src/screens/*"],
      "@state/*": ["src/state/*"],
      "@models/*": ["src/types/*"],
      "@ui/*": ["src/ui/*"]
    }
  }
}
